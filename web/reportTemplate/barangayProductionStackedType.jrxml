	<subDataset name="Report" uuid="b74371f9-5d47-49d4-adcb-0c6c598f71ba">
		<queryString>
			<![CDATA[SELECT PR.SeedType, PR.SeedAcquisition, PR.Season, F.IrrigationMethod, F.LandElevation, B.BarangayName, M.MunicipalityName, SUM(PR.AmountHarvested), SUM(DR.AreaDamaged), SUM(P.PlotSize)
FROM PlantingReport PR JOIN Plot P ON PR.PlotID = P.PlotID 
						JOIN Farm F ON P.FarmID = F.FarmID
                        JOIN Barangay B ON F.BarangayID = B.BarangayID
                        JOIN Municipality M ON B.MunicipalityID = M.MunicipalityID
                        LEFT JOIN DamageIncident DI ON DI.PlantingReportID = PR.PlantingReportID
                        LEFT JOIN DamageReport DR ON DI.DamageIncidentID = DR.DamageIncidentID
                        LEFT JOIN (SELECT DR1.DamageIncidentID, MAX(STR_TO_DATE(DR1.DateReported, '%m-%d-%Y')) AS 'DateReported'
							FROM DamageReport DR1
                            GROUP BY DR1.DamageIncidentID) T1 ON STR_TO_DATE(DR.DateReported, '%m-%d-%Y') = T1.Datereported AND DR.DamageIncidentID = T1.DamageIncidentID
WHERE STR_TO_DATE(CONCAT(PR.HarvestMonth, '-', PR.HarvestDay, '-', PR.HarvestYear), '%m-%d-%Y') > STR_TO_DATE('03-16-2016', '%m-%d-%Y') 
		AND STR_TO_DATE(CONCAT(PR.HarvestMonth, '-', PR.HarvestDay, '-', PR.HarvestYear), '%m-%d-%Y') < STR_TO_DATE('03-15-2017', '%m-%d-%Y') 
GROUP BY PR.SeedType, PR.SeedAcquisition, PR.Season, F.IrrigationMethod, F.LandElevation, B.BarangayName, M.MunicipalityName
HAVING M.MunicipalityName = "San Rafael"]]>
		</queryString>
